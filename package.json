{
  "name": "teams-service",
  "version": "0.0.1",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/app.ts",
    "start:dev": "npx nodemon --watch src --ext ts --exec \"node --inspect=0.0.0.0:9229 -r ts-node/register src/app.ts\"",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "mysql2": "^3.2.0",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
